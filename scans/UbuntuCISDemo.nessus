      fi
      if grep -Pqs -- &apos;^h*net.ipv6.conf.all.disable_ipv6h*=h*1h*(#.*)?$&apos; $l_searchloc &amp;&amp; 
         grep -Pqs -- &apos;^h*net.ipv6.conf.default.disable_ipv6h*=h*1h*(#.*)?$&apos; $l_searchloc &amp;&amp; 
         sysctl net.ipv6.conf.all.disable_ipv6 | grep -Pqs -- &apos;^h*net.ipv6.conf.all.disable_ipv6h*=h*1h*(#.*)?$&apos; &amp;&amp; 
