         l_ipv6s=&apos;disabled&apos;
      fi
      if [ -n &apos;$l_ipv6s&apos; ]; then
         echo -e &apos;
 - IPv6 is disabled on the system, &apos;$l_kpname&apos; is not applicable&apos;
      else
         KPF
      fi
   }
   for l_kpe in $l_parlist; do
      l_kpname=&apos;$(awk -F= &apos;{print $1}&apos; &lt;&lt;&lt; &apos;$l_kpe&apos;)&apos;
      l_kpvalue=&apos;$(awk -F= &apos;{print $2}&apos; &lt;&lt;&lt; &apos;$l_kpe&apos;)&apos;
      if grep -q &apos;^net.ipv6.&apos; &lt;&lt;&lt; &apos;$l_kpe&apos;; then
